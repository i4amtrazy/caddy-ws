{
  "log": {
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log",
    "loglevel": "none"
  },
  "inbounds": [
    {
      "tag": "VLESS-WS",
      "listen": "/dev/shm/vws.sock",
      "protocol": "vless",
      "settings": {
        "decryption": "none",
        "clients": [
          {
            "id": "$UUID",
            "email": "iamtrazy"
          }
        ]
      },
      "streamSettings": {
        "security": "none",
        "network": "ws",
        "wsSettings": {
          "path": "/iamtrazy?ed=2048"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "DIRECT"
    }
  ]
}
